{
  "name": "@typhonjs-fvtt/svelte-standard",
  "version": "0.0.23",
  "description": "Provides a standard Svelte component library for the TyphonJS Runtime Library.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "imports": {
    "#runtime/*": "@typhonjs-fvtt/runtime/*",
    "#standard/*": "@typhonjs-fvtt/svelte-standard/*",
    "#svelte/*": "svelte/*",
    "#svelte": "svelte"
  },
  "exports": {
    "./action": {
      "types": "./_dist/action/index.d.ts",
      "import": "./_dist/action/index.js"
    },
    "./application": {
      "types": "./_dist/application/index.d.ts",
      "import": "./_dist/application/index.js"
    },
    "./component": {
      "import": "./_dist/component/standard/index.js"
    },
    "./component/fvtt": {
      "import": "./_dist/component/fvtt/index.js"
    },
    "./fvtt": {
      "types": "./_dist/fvtt/index.d.ts",
      "import": "./_dist/fvtt/index.js"
    },
    "./plugin/data": {
      "types": "./_dist/plugin/data/index.d.ts",
      "import": "./_dist/plugin/data/index.js"
    },
    "./plugin/system": {
      "types": "./_dist/plugin/system/index.d.ts",
      "import": "./_dist/plugin/system/index.js"
    },
    "./prosemirror": {
      "types": "./_dist/prosemirror/index.d.ts",
      "import": "./_dist/prosemirror/index.js"
    },
    "./store": {
      "types": "./_dist/store/index.d.ts",
      "import": "./_dist/store/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@typhonjs-fvtt/runtime": ">=0.0.22",
    "svelte": ">=3.58.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15",
    "@typhonjs-build-test/esm-d-ts": "^0.0.18",
    "@typhonjs-build-test/rollup-external-imports": "^0.0.1",
    "@typhonjs-config/eslint-config": "^0.6.0",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "@typhonjs-utils/file-util": "^0.0.6",
    "eslint": "^8",
    "fs-extra": "^11",
    "rollup": "^3"
  },
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://github.com/typhonjs-fvtt-lib/svelte-standard#readme",
  "keywords": [
    "foundry vtt",
    "fvtt",
    "svelte"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist",
    "/_types",
    "/src"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-fvtt-lib/svelte-standard",
  "bugs": "https://github.com/typhonjs-fvtt-lib/svelte-standard/issues"
}
