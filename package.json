{
  "name": "@typhonjs-fvtt/svelte-standard",
  "version": "0.0.18",
  "description": "Provides a standard Svelte component library for the TyphonJS Runtime Library.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "svelte": "src/index-svelte.js",
  "exports": {
    "./action": {
      "types": "./_dist/action/index.d.ts",
      "import": "./_dist/action/index.js"
    },
    "./application": {
      "types": "./_dist/application/index.d.ts",
      "import": "./_dist/application/index.js"
    },
    "./component": {
      "types": "./_dist/component/standard/index.d.ts",
      "import": "./_dist/component/standard/index.js"
    },
    "./component/dev": {
      "types": "./_dist/component/dev/index.d.ts",
      "import": "./_dist/component/dev/index.js"
    },
    "./dev-tools/prosemirror": {
      "types": "./_dist/dev-tools/prosemirror/index.d.ts",
      "import": "./_dist/dev-tools/prosemirror/index.js"
    },
    "./plugin/data": {
      "types": "./_dist/plugin/data/index.d.ts",
      "import": "./_dist/plugin/data/index.js"
    },
    "./plugin/system": {
      "types": "./_dist/plugin/system/index.d.ts",
      "import": "./_dist/plugin/system/index.js"
    },
    "./prosemirror/plugins": {
      "types": "./_dist/prosemirror/plugins/index.d.ts",
      "import": "./_dist/prosemirror/plugins/index.js"
    },
    "./store": {
      "types": "./_dist/store/index.d.ts",
      "import": "./_dist/store/index.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#runtime/svelte/application": "@typhonjs-fvtt/runtime/svelte/application",
    "#runtime/color/colord": "@typhonjs-fvtt/runtime/color/colord"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4",
    "@rollup/plugin-commonjs": "^24",
    "@rollup/plugin-node-resolve": "^15",
    "@typhonjs-build-test/esm-d-ts": "^0.0.8",
    "@typhonjs-config/eslint-config": "^0.3.0",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "@typhonjs-utils/file-util": "^0.0.6",
    "eslint": "^8",
    "fs-extra": "^11",
    "prosemirror-dev-toolkit": "^1.0.7",
    "prosemirror-state": "^1.4.2",
    "rollup": "^3",
    "svelte": "3.55.1",
    "upath": "^2"
  },
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://github.com/typhonjs-fvtt/svelte-standard#readme",
  "keywords": [
    "foundry vtt",
    "fvtt",
    "svelte"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist",
    "/_types",
    "/src"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-fvtt/svelte-standard",
  "bugs": "https://github.com/typhonjs-fvtt/svelte-standard/issues"
}
