{
  "name": "@typhonjs-fvtt/svelte-standard",
  "version": "0.0.10",
  "description": "Provides a standard Svelte component library for the TyphonJS Runtime Library.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "svelte": "src/index-svelte.js",
  "exports": {
    "./action": {
      "types": "./_dist/action/index.d.ts",
      "import": "./_dist/action/index.js"
    },
    "./application": {
      "types": "./_dist/application/index.d.ts",
      "import": "./_dist/application/index.js"
    },
    "./component": {
      "types": "./_dist/component/standard/index.d.ts",
      "import": "./_dist/component/standard/index.js"
    },
    "./store": {
      "types": "./_dist/store/index.d.ts",
      "import": "./_dist/store/index.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#internal": "./src/internal/index.js"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3",
    "@rollup/plugin-node-resolve": "^13",
    "@typhonjs-build-test/esm-d-ts": "^0.0.2",
    "@typhonjs-config/eslint-config": "^0.3.0",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "@typhonjs-utils/file-util": "^0.0.6",
    "eslint": "^8",
    "fs-extra": "^10",
    "rollup": "^2",
    "rollup-plugin-sourcemaps": "^0.6.0",
    "rollup-plugin-svelte": "^7",
    "rollup-plugin-terser": "^7",
    "svelte": "3.49.0",
    "upath": "^2"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/typhonjs-fvtt/svelte-standard#readme",
  "keywords": [
    "foundry vtt",
    "fvtt",
    "svelte"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint .",
    "esdoc": "esdoc"
  },
  "files": [
    "/_dist",
    "/_types",
    "/src"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-fvtt/svelte-standard",
  "bugs": "https://github.com/typhonjs-fvtt/svelte-standard/issues"
}
