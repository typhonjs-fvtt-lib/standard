    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "imports": {
    "#runtime/*": "@typhonjs-fvtt/runtime/*",
    "#standard/component/*": {
      "types": "./_dist/component/*/index.d.ts",
      "default": "@typhonjs-fvtt/standard/component/*"
    },
    "#standard/*": "@typhonjs-fvtt/standard/*",
    "#svelte/*": "svelte/*",
    "#svelte": "svelte",
    "#internal/configure": "./src/component/internal/FVTTConfigure.js"
  },
  "exports": {
    "./action/animate/composable": {
      "types": "./_dist/action/animate/composable/index.d.ts",
      "import": "./_dist/action/animate/composable/index.js"
    },
    "./action/animate/composable/ripple": {
      "types": "./_dist/action/animate/composable/ripple/index.d.ts",
      "import": "./_dist/action/animate/composable/ripple/index.js"
    },
    "./application/dialog/document": {
      "types": "./_dist/application/dialog/document/index.d.ts",
      "import": "./_dist/application/dialog/document/index.js"
    },
    "./application/filepicker": {
      "types": "./_dist/application/filepicker/index.d.ts",
      "import": "./_dist/application/filepicker/index.js"
    },
    "./application/menu": {
      "types": "./_dist/application/menu/index.d.ts",
      "import": "./_dist/application/menu/index.js"
    },
    "./application/sidebar": {
      "types": "./_dist/application/sidebar/index.d.ts",
      "import": "./_dist/application/sidebar/index.js"
    },
    "./component/button": {
      "types": "./_dist/component/standard/button/index.d.ts",
      "import": "./_dist/component/standard/button/index.js"
    },
    "./component/color/picker-colord": {
      "types": "./_dist/component/standard/color/picker-colord/index.d.ts",
