{
  "name": "@typhonjs-fvtt/standard",
  "version": "0.2.0-next.5",
  "description": "Provides a standard Svelte component library for the TyphonJS Runtime Library and Foundry VTT.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "imports": {
    "#runtime/*": "@typhonjs-fvtt/runtime/*",
    "#standard/component/*": {
      "types": "./_dist/component/*/index.d.ts",
      "default": "@typhonjs-fvtt/standard/component/*"
    },
    "#standard/*": {
      "types": "./_dist/*/index.d.ts",
      "default": "@typhonjs-fvtt/standard/*"
    },
    "#svelte/*": "svelte/*",
    "#svelte": "svelte",
    "#internal/configure": "./_dist/component/internal/FVTTConfigure.js"
  },
  "exports": {
    "./action/animate/composable": {
      "types": "./_dist/action/animate/composable/index.d.ts",
      "import": "./_dist/action/animate/composable/index.js"
    },
    "./application/control/filepicker": {
      "types": "./_dist/application/control/filepicker/index.d.ts",
      "import": "./_dist/application/control/filepicker/index.js"
    },
    "./application/control/sidebar": {
      "types": "./_dist/application/control/sidebar/index.d.ts",
      "import": "./_dist/application/control/sidebar/index.js"
    },
    "./application/dialog/document": {
      "types": "./_dist/application/dialog/document/index.d.ts",
      "import": "./_dist/application/dialog/document/index.js"
    },
    "./application/menu": {
      "types": "./_dist/application/menu/index.d.ts",
      "import": "./_dist/application/menu/index.js"
    },
    "./component/button": {
      "types": "./_dist/component/standard/button/index.d.ts",
      "import": "./_dist/component/standard/button/index.js"
    },
    "./component/color/picker-colord": {
      "types": "./_dist/component/standard/color/picker-colord/index.d.ts",
      "import": "./_dist/component/standard/color/picker-colord/index.js"
    },
    "./component/container": {
      "types": "./_dist/component/standard/container/index.d.ts",
      "import": "./_dist/component/standard/container/index.js"
    },
    "./component/dom/focus": {
      "types": "./_dist/component/standard/dom/focus/index.d.ts",
      "import": "./_dist/component/standard/dom/focus/index.js"
    },
    "./component/folder": {
      "types": "./_dist/component/standard/folder/index.d.ts",
      "import": "./_dist/component/standard/folder/index.js"
    },
    "./component/form": {
      "types": "./_dist/component/standard/form/index.d.ts",
      "import": "./_dist/component/standard/form/index.js"
    },
    "./component/label": {
      "types": "./_dist/component/standard/label/index.d.ts",
      "import": "./_dist/component/standard/label/index.js"
    },
    "./component/layer/position": {
      "types": "./_dist/component/standard/layer/position/index.d.ts",
      "import": "./_dist/component/standard/layer/position/index.js"
    },
    "./component/layer/side-slide": {
      "types": "./_dist/component/standard/layer/side-slide/index.d.ts",
      "import": "./_dist/component/standard/layer/side-slide/index.js"
    },
    "./component/media": {
      "types": "./_dist/component/standard/media/index.d.ts",
      "import": "./_dist/component/standard/media/index.js"
    },
    "./component/menu": {
      "types": "./_dist/component/standard/menu/index.d.ts",
      "import": "./_dist/component/standard/menu/index.js"
    },
    "./component/fvtt/editor": {
      "types": "./_dist/component/fvtt/editor/index.d.ts",
      "import": "./_dist/component/fvtt/editor/index.js"
    },
    "./component/fvtt/filepicker/button": {
      "types": "./_dist/component/fvtt/filepicker/button/index.d.ts",
      "import": "./_dist/component/fvtt/filepicker/button/index.js"
    },
    "./component/fvtt/settings": {
      "types": "./_dist/component/fvtt/settings/index.d.ts",
      "import": "./_dist/component/fvtt/settings/index.js"
    },
    "./component/fvtt-internal": {
      "types": "./_dist/component/fvtt-internal/index.d.ts",
      "import": "./_dist/component/fvtt-internal/index.js"
    },
    "./fvtt": {
      "types": "./_dist/fvtt/index.d.ts",
      "import": "./_dist/fvtt/index.js"
    },
    "./store/fvtt/settings": {
      "types": "./_dist/store/fvtt/settings/index.d.ts",
      "import": "./_dist/store/fvtt/settings/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@typhonjs-fvtt/runtime": ">=0.2.0-next.4",
    "svelte": ">=4.x.x <5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@typhonjs-build-test/esm-d-ts": "^0.3.0-next.9",
    "@typhonjs-build-test/esm-d-ts-plugin-svelte": "^0.1.0-next.1",
    "@typhonjs-build-test/rollup-plugin-pkg-imports": "^0.5.0",
    "@typhonjs-config/eslint-config": "^0.6.3",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "@typhonjs-svelte/standard-base": "github:typhonjs-svelte/standard-base#0.2.0-next.2",
    "@typhonjs-utils/file-util": "^0.1.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "rollup": "^4.22.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/typhonjs-fvtt-lib/standard#readme",
  "keywords": [
    "foundry vtt",
    "fvtt",
    "svelte",
    "typhonjs"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-fvtt-lib/standard",
  "bugs": "https://github.com/typhonjs-fvtt-lib/standard/issues"
}
