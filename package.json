    },
    "./component/fvtt-internal": {
      "import": "./_dist/component/fvtt-internal/index.js"
    },
    "./fvtt": {
      "types": "./_dist/fvtt/index.d.ts",
      "import": "./_dist/fvtt/index.js"
    },
    "./store/fvtt/settings": {
      "types": "./_dist/store/fvtt/settings/index.d.ts",
      "import": "./_dist/store/fvtt/settings/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@typhonjs-fvtt/runtime": ">=0.1.0",
    "svelte": ">=4.x.x <5"
  },
  "dependencies": {
    "@typhonjs-svelte/standard-base": "github:typhonjs-svelte/standard-base"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@typhonjs-build-test/esm-d-ts": "^0.3.0-next.8",
    "@typhonjs-build-test/esm-d-ts-plugin-svelte": "^0.1.0-next.1",
    "@typhonjs-build-test/rollup-plugin-pkg-imports": "^0.5.0",
    "@typhonjs-config/eslint-config": "^0.6.3",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "@typhonjs-utils/file-util": "^0.1.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "rollup": "^4.22.4"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/typhonjs-fvtt-lib/svelte-standard#readme",
  "keywords": [
    "foundry vtt",
    "fvtt",
    "svelte"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-fvtt-lib/svelte-standard",
  "bugs": "https://github.com/typhonjs-fvtt-lib/svelte-standard/issues"
}
