    },
    "./component/container": {
      "types": "./_dist/component/standard/container/index.d.ts",
      "import": "./_dist/component/standard/container/index.js"
    },
    "./component/dom": {
      "types": "./_dist/component/standard/dom/index.d.ts",
      "import": "./_dist/component/standard/dom/index.js"
